{
  "name": "console-enhanced",
  "version": "0.2.1",
  "description": "Enhanced console logging with automatic variable names, file locations, timestamps, and performance measurement",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "test": "node dist/test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "debug",
    "console",
    "logging",
    "logger",
    "timestamp",
    "callsite",
    "line-number",
    "performance",
    "measure",
    "typescript"
  ],
  "author": "donqlee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/donqlee/console-enhanced.git"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
